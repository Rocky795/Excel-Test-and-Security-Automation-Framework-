{
  "Username Field": "#username",
  "Password Field": "#password",
  "Login Button": "xpath=//input[@name='Login']",
  "App Launcher": "xpath=//button[@title='App Launcher']",
  "Sales App": "//a[@role='option' and @data-label='Sales']",
  "Search Apps": "//input[@placeholder='Search apps and items...']",
  "Opportunity Tab": "xpath=//a[@title='Opportunities']",
  "New Opportunity Button":"xpath=//a[@title='New']",
  "Opportunity Name": "//input[@name='Name']",
  "Opportunity Stage": "(//button[@data-value=\"--None--\"])[1]",
  "Opportunity Close Date": "//input[@name='CloseDate']",
  "Opportunity Stage Select Qualification": "//lightning-base-combobox-item[@data-value='Qualification' and .//span[@title='Qualification']]",
  "Opportunity Save": "xpath=//button[@name='SaveEdit' and text()='Save']",
    "record_link_by_id": "xpath=//lightning-formatted-text[text()='${record_title}']",
  "Leads Link": "//p[@class='slds-truncate' and text()='Leads']",
  "Accounts Link": "//p[@class='slds-truncate' and text()='Accounts']",
  "Contacts Link": "//p[@class='slds-truncate' and text()='Contacts']",
  "Opportunities Link": "//p[@class='slds-truncate' and text()='Opportunities']",
  "New Button": "//div[@title='New' or contains(@class, 'forceActionLink') and .//div[text()='New']]",
  "Save Button": "//button[text()='Save' or @name='SaveEdit']",
  "Cancel Button": "//button[text()='Cancel']",
  "Edit Button": "//button[text()='Edit']",
  "Delete Button": "//button[text()='Delete']",
  "First Name Field": "//input[contains(@placeholder, 'First Name') or contains(@name, 'firstName')]",
  "Last Name Field": "//input[contains(@placeholder, 'Last Name') or contains(@name, 'lastName')]",
  "Company Field": "//input[contains(@placeholder, 'Company') or contains(@name, 'company')]",
  "Email Field": "//input[contains(@placeholder, 'Email') or contains(@name, 'email')]",
  "Phone Field": "//input[contains(@placeholder, 'Phone') or contains(@name, 'phone')]"
}